package Ilay;
import java.util.Scanner;
public class pratik20 {

	public static void main(String[] args) {
		//Üslü sayıları hesaplayan program.
		int carpim=1, sayi1, sayi2;
		Scanner girdi= new Scanner(System.in);
		System.out.println("Üs almak istediğiniz sayıyı giriniz.");
		sayi1=girdi.nextInt();
		System.out.println("Üssü için bir sayı giriniz.");
		sayi2=girdi.nextInt();
		 for (int i = 0; i < sayi2; i++) {
             carpim= carpim*sayi1;

	}	System.out.println("Sonuç: "+ carpim);

}
}
